generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URI")
}

model Player {
  id        String @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  discordId String @unique @map("discord_id")
  money     Int    @default(0)
  stars     Int    @default(0)
}
